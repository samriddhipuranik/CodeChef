import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int N=0;
		int T=sc.nextInt();
		for(int i=1;i<=T;i++){
		    N=sc.nextInt();
		    if(N%2==0)
		    System.out.println("Yes");
		    else
		    System.out.println("No");

		}
	}
}
