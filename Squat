import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		//no. of test case
		int T=sc.nextInt();
		int temp=0;
		
		for(int i=1;i<=T;i++){
		    temp=sc.nextInt();
		
		int X=temp*15;
		System.out.println(X);
		}
	}
}
